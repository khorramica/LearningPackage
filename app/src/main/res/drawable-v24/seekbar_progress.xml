<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:id="@+id/progressshape" >
        <clip>
            <shape
                android:shape="rectangle" >
                <size android:height="55dp"/>
                <corners
                    android:radius="55dp" />
<!--                <solid android:color="#0F5702"/>-->
                <gradient
                    android:angle="270"
                    android:centerY="0.2"
                    android:startColor="#0F5702"
                    android:centerColor="#C82DCC10"
                    android:endColor="#C82DCC10"
                    android:type="linear" />
            </shape>
        </clip>
    </item>
</layer-list>