<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ir.khorrami.learningpackage">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearningPackage">
        <activity
            android:name=".NavBar.NavBarActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RxJava.BehaviorRelay.RxJava_BehaviorRelayActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".RxJava.PublishRelay.RxJava_PublishRelayActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".Login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Permission.PermissionActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".Room.RoomDbActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".Navigation.NavigationActivity"
            android:exported="true" />
        <activity
            android:name=".Retrofit.RetrofitActivity"
            android:exported="true" />
        <activity
            android:name=".RxJava.Just.RxJava_JustActivity"
            android:exported="true" />
        <activity
            android:name=".RxJava.Range.RxJava_RangeActivity"
            android:exported="true" />
        <activity
            android:name=".RxJava.Create.RxJava_Create"
            android:exported="true" />
        <activity
            android:name=".MVVM.Heros.view.ShowHerosActivity"
            android:exported="true" />
    </application>

</manifest>